	$(TEXI2POD) $(srcdir)/wget.texi $@

$(MAN): wget.pod
	$(POD2MAN) --center="GNU Wget" --release="GNU Wget 1.12" $? > $@

#wget.cat: $(MAN)
#	nroff -man $? > $@

wget_us.ps: wget.dvi
	$(DVIPS) -t letter -o $@ wget.dvi

